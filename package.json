        "deepmerge": "^2.2.1",
        "google-protobuf": "^3.6.1",
        "lodash": "^4.17.11",
        "md5.js": "^1.3.5",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-keydown": "^1.9.7",
        "react-localization": "^1.0.13",
        "react-redux": "^5.1.1",
        "react-router-dom": "^4.3.1",
        "react-scripts": "2.1.1",
        "react-virtualized": "^9.21.0",
        "reactstrap": "^6.5.0",
        "redux": "^4.0.1",
        "redux-immutable-state-invariant": "^2.1.0",
